    "@babel/core": "7.2.2",
    "@babel/preset-env": "7.3.1",
    "@babel/preset-react": "7.0.0",
    "babel-jest": "24.0.0",
    "babel-loader": "8.3.0",
    "css-loader": "6.7.3",
    "file-loader": "6.2.0",
    "html-loader": "4.2.0",
    "image-webpack-loader": "8.1.0",
    "jest": "24.0.0",
    "redux-mock-store": "1.5.3",
    "sinon": "7.2.3",
    "style-loader": "3.3.1",
    "webpack": "5.75.0",
    "webpack-cli": "5.0.1",
    "webpack-dev-server": "3.1.14"
  },
  "dependencies": {
    "@babel/cli": "7.2.3",
